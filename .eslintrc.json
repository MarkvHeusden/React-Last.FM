{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier", "react"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "prettier/prettier": 0,
        "react/prop-types": 0
    }
}
